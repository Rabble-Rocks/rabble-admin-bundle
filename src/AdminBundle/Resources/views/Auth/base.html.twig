{% block document %}
    <!doctype html>
    <html>
    <head>
        {% block head %}
            <meta charset="utf-8">
            <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
            <link rel="icon" href="{{ asset('admin/build/assets/static/images/logo.png') }}" type="image/png" />
            <title>
                {% block title %}{% endblock %}
                {% if titleBrand|default %}
                    | {{ titleBrand }}
                {% endif %}
            </title>
            {% include '@RabbleAdmin/Layout/loader.html.twig' %}
            {% block css %}
                <link href="{{ asset('admin/build/style.css') }}" rel="stylesheet">
            {% endblock %}
        {% endblock %}
    </head>
    {% block body_outer %}
        <body class="app">
        {% block body %}
            <div id="loader">
                <div class="spinner"></div>
            </div>
            <script>window.addEventListener('load', function load() {
                    const loader = document.getElementById('loader');
                    setTimeout(function() {
                        loader.classList.add('fadeOut');
                    }, 300);
                });
            </script>
            <div class="peers ai-s fxw-nw h-100vh">
                <div class="d-n@sm- peer peer-greed h-100 pos-r bgr-n bgpX-c bgpY-c bgsz-cv" style="background-image:url({{ asset('admin/build/assets/static/images/bg.jpg') }})">
                    <div class="pos-a centerXY">
                        <div class="bgc-white bdrs-50p pos-r" style="width:120px;height:120px"><img class="pos-a centerXY img-fluid" src="{{ asset('admin/build/assets/static/images/logo.png') }}" style="max-width: 45px" alt="logo"></div>
                    </div>
                </div>
                <div class="col-12 col-md-4 peer pX-40 pY-80 h-100 bgc-white scrollable pos-r" style="min-width:320px">
                    {% block page_content %}{% endblock %}
                </div>
            </div>
            <script type="text/javascript" src="{{ asset('admin/build/bundle.js') }}"></script>
        {% endblock %}
        </body>
    {% endblock %}
    </html>
{% endblock %}